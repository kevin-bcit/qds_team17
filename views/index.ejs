<%- include("templates/header") %> 
<% if (username) { %>
    <h1>Hello, <%- username %>!</h1>
    <a href="/members">
        <button>Go to Members Area</button>
    </a>
    <br>
    <a href="/logout">
        <button>Logout</button>
    </a>
<% } else { %> 
    <a href="/signup">
        <button>Sign up</button>
    </a>
    <br>
    <a href="/login">
        <button>Log in</button>
    </a>
<% } %>  
<%- include("templates/footer") %>